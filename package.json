{
  "name": "starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dvr": "docker volume rm supabase_db_starter supabase_config_starter supabase_storage_starter supabase_edge_runtime_starter supabase_inbucket_starter",
    "supareset": "supabase db reset",
    "supareset:test": "supabase db reset && supabase test db --debug",
    "supastart": "supabase start --ignore-health-check",
    "supastop": "supabase stop && npm run dvr",
    "supastop:backup": "supabase stop --backup",
    "types": "supabase gen types typescript --local > src/lib/supabase/types.ts",
    "supatest": "supabase test db",
    "format": "prettier --write . --log-level warn",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@edge-csrf/nextjs": "^2.5.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@t3-oss/env-nextjs": "^0.11.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.446.0",
    "next": "14.2.13",
    "next-safe-action": "^7.9.3",
    "next-themes": "^0.3.0",
    "pino": "^9.4.0",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.13",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "pino-pretty": "^11.2.2",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "supabase": "^1.200.3",
    "tailwindcss": "^3.4.1",
    "tsc-files": "^1.1.4",
    "typescript": "^5"
  }
}
